@font-face {
  font-family: "museo";
  src: url('<%= asset_url 'museo300.otf' %>');
  font-weight: 300;
  font-style: normal;
}

@font-face {
  font-family: "museo";
  src: url('<%= asset_url 'museo500.otf' %>');
  font-weight: 500;
  font-style: normal;
}

@font-face {
  font-family: "museo";
  src: url('<%= asset_url 'museo700.otf' %>');
  font-weight: 700;
  font-style: normal;
}

button, a, p, h1, h2, h3 {
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
